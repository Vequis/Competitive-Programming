Busca(V, chave)
    for i = 1 to V.tamanho
        if V[i] == chave
            return i
    return NIL

BuscaBinaria(V, chave)
    ini = 1
    fim = V.tamanho+1
    while ini != fim
        meio = (ini + fim)/2
        if V[meio] == chave 
            return meio
        else if V[meio] > chave
            fim = meio
        else
            ini = meio+1
    
    return NIL

