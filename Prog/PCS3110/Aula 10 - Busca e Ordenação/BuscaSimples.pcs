            direita = meio-1

InsertionSort(V)
    for j = 2 to V.tamanho
        i = j - 1
        chave = A[j]
        while i > 0 and A[i] > chave
            A[i+1] = A[i]
            i--
        A[i+1] = chave

SelectionSort(V)
    for i = 1 to V.tamanho - 1
        menor = i
        for j= i + 1 to V.tamanho
            if A[j] < A[menor]
                menor = j
        t = A[menor]
        A[menor] = A[i]
        A[i] = t
